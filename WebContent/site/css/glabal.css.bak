.body{ min-width: 1200px; background: url(bg_top.jpg) no-repeat center top;}
.main{ background: url(bg_btm.jpg) no-repeat center bottom; min-height:850px; position:relative; z-index: 99;}
.top{ width: 1125px; margin: 0 auto; height: 80px; padding-top: 35px;}
.search{ width: 257px; height: 33px; line-height: 33px; background: #e3f2fd; border: 1px solid #2c80da; border-radius:18px; margin-top:30px;}
.search input{ width: 200px; margin: 3px 0 0 15px; height:24px; line-height:24px; background: #e3f2fd; border:1px solid #e3f2fd; font-size: 14px; color: #3283da; outline: none;}
