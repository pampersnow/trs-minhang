.nav_box{ width: 1297px;height: 98px; margin: 0 auto; position: relative;}
.nav{height:58px; width: 1125px; margin: 0 auto; line-height: 52px; overflow: visible;background: url(nav_center.jpg) repeat-x left top; position: relative;}
.nav_l{ position: absolute; left:0px; top: 0;}
