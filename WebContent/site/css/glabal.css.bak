.nav ul{ overflow: visible; }
.nav li{ float:left;position:relative;background: url(nav_li.jpg) no-repeat left top; overflow:visible;height:58px; ;font-size:20px; width: 145px; text-align: center;}
.nav li.home{ background: none; width: 110px;}
