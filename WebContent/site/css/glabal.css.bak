.nav li:hover a{font-size: 18px; font-weight: bold; color: #ff931b; background:url(nav_center.png) no-repeat center bottom;}
.nav li.home:hover a{ background: none; }
.nav li .subChannel{ text-align: center; position: absolute; z-index: 100; /*width: 1000px;*/visibility: hidden;   /*background: #fff; padding-left: 20px;*/}
.nav li .subChannel a{ color: #000;font-size: 16px; margin-right: 34px; height: 36px; line-height: 36px; float: left; font-weight: normal;  }
.nav li:hover .subChannel a{ background:none;}
.nav li:hover>.subChannel{visibility: visible; }

